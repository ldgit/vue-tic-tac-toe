{
  "name": "tic-tac-toe-vue",
  "version": "1.0.0",
  "description": "Ultimate tic-tac-toe in Vue",
  "homepage": "https://ldgit.github.io/vue-tic-tac-toe/",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "webpack-dev-server --open --mode=development",
    "lint": "eslint --ext .vue,.js ./",
    "test:all": "npm run lint && npm t",
    "deploy": "npm run test:all && npm run build:prod && rm -rf docs/* && cp -R dist/. docs/"
  },
  "author": "Danko Lučić",
  "license": "MIT",
  "dependencies": {
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^5.0.0-beta.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "style-loader": "^0.23.1",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
}
